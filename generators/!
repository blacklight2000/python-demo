""" fizzbuzz """


def fb(n_value):
    """ fizzbuzz starts from 1 """
    for i_value in range(1, n_value):
        if i_value % 15 == 0:
            print "FizzBuzz %s"  % i_value
        elif i_value % 3 == 0:
            print "Fizz %d" % i_value
        elif i_value % 5 == 0:
            print "Buzz %d" % i_value
        else:
            print i_value
        i_value = i_value + 1


if __name__ == "__main__":
    print fb(100)
